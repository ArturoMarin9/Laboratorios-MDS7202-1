        "        objective='reg:squarederror',  # Establecer el objetivo como 'reg:squarederror'\n",
        "        **params,  # Pasar los hiperpar치metros como kwargs\n",
        "        n_jobs=-1,\n",
        "        random_state=seed,\n",
        "        eval_metric='mae'  # Establecer la m칠trica de evaluaci칩n como 'mae'\n",
        "    )\n",
        "\n",
        "    # Crear un pipeline con los pasos necesarios\n",
